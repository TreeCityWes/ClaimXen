{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "TSC_COMPILE_ON_ERROR": "true",
    "GENERATE_SOURCEMAP": "false",
    "CI": "false"
  }
} 